						<label>Телефон</label>
						<div class="phone__input__wrapper">
							<input type="tel" name="phone" placeholder="000 000 00 00">
						</div>
						<label>Пароль</label>
						<input type="password" name="password" placeholder="Придумайте пароль">
						<label>Повторите пароль</label>
						<input class="reg-form__input-password" type="password" name="password" placeholder="Повторите пароль">
						<div class="checkbox-form">
							<input class="checkbox__input" type="checkbox" id="reg-form__check">
							<label for="reg-form__check" class="checkbox__label">Я согласен на обработку персональных данных</label>
						</div>
					</form>
					<div class="form__buttons">
