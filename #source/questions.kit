						<label>Фамилия</label>
						<input type="text" name="lastname" placeholder="Введите вашу фамилию">
						<label>Отчество</label>
						<input class="_error" type="text" name="patronymic" placeholder="Введите отчество">
					</form>
					<button class="form__green__btn" id="to-reg-2">Продолжить</button>
					<div class="check__account">
